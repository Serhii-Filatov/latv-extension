{
  "manifest_version": 3,
  "name": "LivingAboard.tv",
  "description": "Watching your favorite TV Series",
  "version": "1.0.0",
  "action": {
    "default_popup": "./popup.html",
    "default_icon": "./images/logo.png"
  },

  "permissions": [
    "proxy",
    "tabs"
  ],
  
  "background": {
    "service_worker": "background.js"
  }
}